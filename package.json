{
  "name": "google-books-react-search",
  "version": "1.0.0",
  "description": "<h1 align=\"center\">Google Books React Search</h1>",
  "main": "server.js",
 
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fidel-bello/google-books-react-search.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fidel-bello/google-books-react-search/issues"
  },
  "homepage": "https://github.com/fidel-bello/google-books-react-search#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.10",
    "morgan": "^1.10.0"
  }
}
